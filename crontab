# Descargar imágenes cada 5 minutos
*/5 * * * * /home/mint/Desktop/repos/vigo-camera-downloader-py/download.sh >> /tmp/download.log 2>&1

# Procesar día anterior y subir a Drive diariamente a las 3 AM
0 3 * * * /home/mint/Desktop/repos/vigo-camera-downloader-py/sync.sh >> /tmp/sync.log 2>&1

# Empaquetar mes anterior el día 1 de cada mes a las 7 AM
0 7 1 * * /home/mint/Desktop/repos/vigo-camera-downloader-py/pack_last_month.sh >> /tmp/pack_month.log 2>&1